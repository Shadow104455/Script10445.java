            subMenteIA();
        } catch (Exception e) {
            System.err.println("Erro na IA auxiliar: " + e.getMessage());
            autoReparar("iniciarIAAuxiliar");
        }
    }

    // Submente IA para adapta√ß√£o e evolu√ß√£o cont√≠nua
    public static void subMenteIA() {
        System.out.println("[SubIA] Ativada! Escaneando ambiente e ajustando par√¢metros...");
        try {
            for (int i = 0; i < 10; i++) {
                System.out.println("[SubIA] Refinando conex√µes e recalibrando l√≥gica qu√¢ntica...");
                Thread.sleep(500);
